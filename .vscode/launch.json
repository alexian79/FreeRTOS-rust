{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "./target/thumbv7em-none-eabihf/debug/examples/atmel",
            "name": "Debug Microcontroller",
            "request": "attach",
            "type": "cortex-debug",
            "servertype": "external",
            "device": "Cortex-M7",
            "gdbTarget": "localhost:2331",
            "showDevDebugOutput": true,
            "demangle": false,
            "preLaunchTask": "Cargo Build (debug)",
            "debuggerArgs": ["-x", "jlink.gdb"]
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug example 'atmel'",
            "cargo": {
                "args": [
                    "build",
                    "--example=atmel",
                    "--package=freertos-rust-examples"
                ],
                "filter": {
                    "name": "atmel",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
    ]
}